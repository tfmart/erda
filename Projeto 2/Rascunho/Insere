#include<stdio.h>
#include<stdlib.h>
#include<string.h>


struct no {
    char chave[20], palavra[20];
    struct no *dir, *esq;
    int freq;
};


typedef struct no celula;

int freq(celula **raiz){
int freq=1;

freq=freq+(*raiz)->freq;
freq++;
(*raiz)->freq=freq;

	return ((*raiz)->freq);


}

int insere(celula **raiz, char palavra[]) {
	
    celula *aux=(*raiz);
    
    if ((*raiz) == NULL)
        
    {
        (*raiz) = (celula *)malloc(sizeof(celula));
        strcpy((*raiz)->chave, palavra);
        (*raiz)->esq = NULL;
        (*raiz)->dir = NULL;
        return 1;
    }
    
    else if (strcmp((*raiz)->chave, palavra) == 0){

	

        return (freq( &(*raiz)));

    }
    
    
    else if (strcmp(palavra, (*raiz)->chave) > 0){ 

        return(insere(&(*raiz)->dir, palavra));
    }
    
    
    else if (strcmp(palavra, (*raiz)->chave) < 0){
        return (insere(&(*raiz)->esq, palavra));
    }
    
}


int main(int argc,char *argv[]) {
    
    celula *lista = NULL;
    int op, resposta;
    char palavra[20];
                   
while((scanf ("%s", palavra)) != EOF){

resposta=insere(&lista,palavra);

 printf(" %s %d\n",palavra,resposta);

}

               
                
                               
                system("pause");
               
        
        
        
   

    return 0;
}
